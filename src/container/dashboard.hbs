<html>

  <head>
    <title>
      Dashboard page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    {{! <link href='css/normalize.css' rel='stylesheet' /> }}

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>
    <div class='page page--dashboard'>
      <aside
        class='page__aside aside vertical vertical--big'
      >
        <div class='text aside__logo'>
          Sidebar
        </div>
        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/home.svg);'
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-button click2'>
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/orders.svg);'
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-button click2'>
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/products.svg);'
            ></span>
            <span class='text text--light'>Products</span>
          </a>

          <div class='relative click'>
            <div class='field field--select'>
              <span class='text text--light'>Choose</span>
            </div>
            <select class='field__select'>
              <option value='dog'>Dog</option>
              <option value='cat'>Cat</option>
              <option value='hamster'>Hamster</option>
              <option value='parrot'>Parrot</option>
              <option value='spider'>Spider</option>
            </select>
          </div>
        </nav>
      </aside>
      <header class='header'>
        <div class='layout header__content text--small'>
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style='background-image: url(/svg/header/search.svg);'
            ></span>
            <span
              class='icon icon--header-settings icon--button icon--color-blue click'
              style='background-image: url(/svg/header/settings.svg);'
            ></span>
          </div>

          <div class='header__icons-block'>
            <span
              class='icon header--icons icon--button icon--color-yellow click'
              style='background-image: url(/svg/header/antenna.svg);'
            ></span>
            <span
              class='icon header--icons icon--button icon--color-sky click'
              style='background-image: url(/svg/header/symbols.svg);'
            ></span>
            <span
              class='icon header--icons icon--button icon--color-yellow click'
              style='background-image: url(/svg/header/bell.svg);'
            ></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div
              class='analytic analytic--double analytic--blue-dark'
            >
              <span
                class='icon analytic__button icon--button icon--color-purple click'
                style='background-image: url(/svg/analytic/earning.svg);'
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$500</div>
                <div
                  class='text text--small text--gray'
                >Total earning</div>
                <span
                  class='icon analytic__arrow'
                  style='background-image: url(/svg/analytic/blue-arrow.svg);'
                ></span>
              </div>
            </div>

            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style='background-image: url(/svg/analytic/order.svg);'
              ></span>

              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$961</div>
                <div
                  class='text text--small text--gray'
                >Total Order</div>
              </div>
            </div>

            <div class='analytic_small analytic--blue'>
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style='background-image: url(/svg/analytic/earning.svg);'
              ></span>

              <div
                class='relative horizontal horizontal--small'
              >
                <div
                  class='analytic_small__title text text--light'
                >$203k</div>
                <div
                  class='text text--small text--gray'
                >Total Income</div>
              </div>
            </div>

            <div class='analytic_small analytic--light'>
              <span
                class='icon analytic__button icon--button icon--color-yellow click'
                style='background-image: url(/svg/analytic/shop.svg);'
              ></span>

              <div
                class='relative horizontal horizontal--small'
              >
                <div
                  class='analytic_small__title text'
                >$203k</div>
                <div
                  class='text text--small text--gray'
                >Total Income</div>
              </div>
            </div>
          </section>

          <section class='customer'>
            <div class='box customer__block'>
              <div class='text--title'>Purchase list</div>
              <div class='divider'></div>
              <div class='purchase__element'>
                <div class='text'>ID437661</div>
                <div class='text'>AirOn divroCam</div>
                <div class='text'>132.32 EUR</div>
                <div class='text'>Customer</div>
                <div class='text'>May 11, 2023, 14:21</div>
                <div class='text'>7/8 USB Type-C</div>
                <div class='text text--red'>Not paid</div>
                <div class='text'>robin@com.de</div>
              </div>
              <div class='divider'></div>
              <div class='purchase__element'>
                <div class='text'>ID437661</div>
                <div class='text'>PHILIPS Essential</div>
                <div class='text'>543 USD</div>
                <div class='text'>Customer</div>
                <div class='text'>May 11, 2023, 14:21</div>
                <div class='text'>HD9200/90</div>
                <div class='text text--green'>Pay by Stripe</div>
                <div class='text'>dima@com.ua</div>
              </div>
            </div>

            <div class='box customer__block payment'>
              <div class='text--title'>Payment</div>
              <div class='divider'></div>
              <div class='radio__block'>
                <div>
                  <input
                    type='radio'
                    name='type_card'
                    value='Credit card'
                    checked
                  />
                  <label>Credit card</label>
                </div>

                <div>
                  <input
                    type='radio'
                    name='type_card'
                    value='Debit card'
                  />
                  <label>Debit card</label>
                </div>

                <div>
                  <input
                    type='radio'
                    name='type_card'
                    value='PayPal'
                  />
                  <label>PayPal</label>
                </div>
              </div>

              <div class='payment__element'>
                <div class='input__title'>Name on card</div>
                <div class='input__title'>Credit card number</div>
                <input
                  class='input__field'
                  type='text'
                  name='name on card'
                />
                <input
                  class='input__field'
                  type='text'
                  name='credit card number'
                />
              </div>
              <div class='payment__element'>
                <div class='input__title'>Expiration</div>
                <div class='input__title'>CVV</div>
                <input
                  class='input__field'
                  type='text'
                  name='expiration'
                />
                <input
                  class='input__field'
                  type='text'
                  name='cvv'
                />
              </div>

              <div class='divider'></div>

              <div class='payment__element'>
                <div class='input__title'>Country</div>
                <div class='input__title'>State</div>
                <div class='relative click'>
                  <div class='field field--select'>
                    <span class='text'>Choose</span>
                  </div>
                  <select class='field__select'>
                    <option value='dog'>Dog</option>
                    <option value='cat'>Cat</option>
                    <option value='hamster'>Hamster</option>
                    <option value='parrot'>Parrot</option>
                    <option value='spider'>Spider</option>
                  </select>
                  <span
                    class='icon select__icon icon--button click'
                    style='background-image: url(/svg/arrow.svg);'
                  ></span>
                </div>

                <div class='relative click'>
                  <div class='field field--select'>
                    <span class='text'>Choose</span>
                  </div>
                  <select class='field__select'>
                    <option value='dog'>Dog</option>
                    <option value='cat'>Cat</option>
                    <option value='hamster'>Hamster</option>
                    <option value='parrot'>Parrot</option>
                    <option value='spider'>Spider</option>
                  </select>
                  <span
                    class='icon select__icon icon--button click'
                    style='background-image: url(/svg/arrow.svg);'
                  ></span>
                </div>
              </div>

              <div class='divider'></div>

              <a
                href='#'
                class='nav-button nav-button--active click text--center'
              >
                <div class='text text--light'>Continue to
                  checkout</div>
              </a>
            </div>
            {{! <div class="">text</div>
				<div class="">text</div> }}
          </section>
        </div>
      </main>

    </div>
  </body>

</html>